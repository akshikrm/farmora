{
    "name": "faarm",
    "version": "1.0.0",
    "type": "module",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "nodemon app.js",
        "dev": "nodemon app.js",
        "doc": "jsdoc ./src/ -r",
        "db:migrate": "sequelize-cli db:migrate",
        "db:migrate:undo": "sequelize-cli db:migrate:undo",
        "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
        "db:seed": "sequelize-cli db:seed:all",
        "db:seed:undo": "sequelize-cli db:seed:undo",
        "db:seed:undo:all": "sequelize-cli db:seed:undo:all",
        "db:refresh": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed"
    },
    "imports": {
        "#routes/*": "./src/routes/*.js",
        "#utils/*": "./src/utils/*.js",
        "#controllers/*": "./src/controllers/*.js",
        "#middlewares/*": "./src/middlewares/*.js",
        "#validators/*": "./src/validators/*.js",
        "#models/*": "./models/*.js",
        "#services/*": "./src/services/*.js",
        "#errors/*": "./src/errors/*.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "fs": "^0.0.1-security",
        "handlebars": "^4.7.8",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^6.10.0",
        "pg": "^8.13.1",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.5",
        "sequelize-cli": "^6.6.2",
        "uuid": "^8.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.38.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/uuid": "^10.0.0",
        "eslint": "^9.38.0",
        "globals": "^16.4.0",
        "jsdoc": "^4.0.5",
        "nodemon": "^3.1.9"
    }
}
